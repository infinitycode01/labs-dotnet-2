@page
@model Lab3_AspNet_EF.Pages.Calls.CreateModel

<h2>Create Call</h2>

<form method="post">
    <div class="form-group">
        <label asp-for="Call.PhoneId"></label>
        <input asp-for="Call.PhoneId" class="form-control" />
        <span asp-validation-for="Call.PhoneId" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Call.CityId"></label>
        <input asp-for="Call.CityId" class="form-control" />
        <span asp-validation-for="Call.CityId" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Call.Date"></label>
        <input asp-for="Call.Date" class="form-control" />
        <span asp-validation-for="Call.Date" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Call.Duration"></label>
        <input asp-for="Call.Duration" class="form-control" />
        <span asp-validation-for="Call.Duration" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Call.TariffId"></label>
        <input asp-for="Call.TariffId" class="form-control" />
        <span asp-validation-for="Call.TariffId" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Create</button>
</form>
